# Ollama API设置
OLLAMA_API_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5-coder:3b
OLLAMA_TIMEOUT=60

# 模型参数设置
MODEL_TEMPERATURE=0.01
MODEL_TOP_P=0.9
PRECISION_BIAS=0.0

# 输出设置
OUTPUT_DIR=outputs
DELAY=0.1

# 日志设置
LOG_LEVEL=INFO
# LOG_FILE=logs/app.log

# 功能开关
SAVE_SUMMARY=true
RESUME_FROM_CHECKPOINT=true
SAVE_RAW_RESPONSE=false
GENERATE_REPORT=true
OPEN_REPORT=false
